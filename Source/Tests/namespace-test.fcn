
/*
This is to test the namespacing
*/

:verbose True
:file "this/that/other.txt"
:verbosity 4

`x = [1, 2, 3]`
`y = [4, 5, 6]`

Domain x Reals
Domain y Reals 4 5
Domain z Naturals 1 2 -nrandom 10

// this exist outside any namespace/unittest

Assert fn:
  | :ignore-True yes  :message "This is a test of some random stuff"
  | (1,2,3) > 4
  | (1, 2, three=3, four=4) ≠ 5
  | (1, `[2.0]`, three=3.0) float?
  ;


Test fn3 x,y:
  | :set setter  :get 3
  | modof? 10
  | is-a? float
  | same-as? `[10,10,10]`
  ;

Domain X Reals 0 100
Domain Y Reals 0 100
Domain Z Reals 0 100

Test logical X,Y,Z:
  | :method art -dist [0 0 0]
  | < 4 ∨ between 0 100
  | modof? 10
  | between? 0 100
  | is-int? ∧ is-float?
  | !int? ∧ !float?
  | (int? ∧ float?)
  | !(int? ∨ float?)
  | !is-int? ∧ (is-float? ∨ is-fraction?)
  | ! is-int? ∧ !(> 8 ∧ < 10 ∧ = 9)
  | is-int? ∨ is-float? ∨ !fraction? ∧ gt? 0
  ;

ns-t1a {

   // set some directives
   :name ns_test_1
   :desc "Tests fn over D"
   :enumerate True
   :verbose 4

   `x = 4`

   Test fn D:
     | ≠ 4
     | is? 'a'
     | even? one
     | > 2
     | == 5.0
     | = -1
     | == a
     ;
}


ns-1a {

   :test yes
   :verbose yes
   :level x
   :file "../../file.fcn"

   Test fn domain:
     | :set yes :do thing
     | `x = 3`
     | ≠  4
     | >  5
     | == 5.0
     | = "asdf"
     | = `[1,2,3]`
     | ± 7
     | between 0 10
     | any? str int
     ;

     `x = 1`

     x := 2
     y := `List[str]` : `[1,2,3]`
     z := str : "string!"

    Assert fn:
      | :do thing :are-sure yes
      | (1,2,3) == 2.0
      ;

    Test fn Some-Domain:
      | ==  4
      | is? int
      | ==  `[1,2,3]`
      | ≠   z
      | ≠   "a"
      ;

    }


